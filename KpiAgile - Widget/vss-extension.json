{
    "manifestVersion": 1,
    "id": "lead-time-extension",
    "version": "1.0.0",
    "name": "Lead Time Metric",
    "description": "Get the Lead Time of a set of Work Items in a specific query",
    "icons": {
        "default": "img/Time-Machine-icon.png"
    },
    "categories": [
        "Plan and track"
    ],
    "tags": [
        "working",
        "lead time",
        "cycle time",
        "throughput",
        "agile",
        "kanban",
        "lean"
    ],
    "branding": {
        "color": "rgb(116, 51, 147)",
        "theme": "dark"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "publisher": "brunosouza",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "contributions": [{
        "id": "LeadTimeMetric",
        "type": "ms.vss-dashboards-web.widget",
        "targets": [
            "ms.vss-dashboards-web.widget-catalog",
            ".LeadTimeMetric.Configuration"
        ],
        "properties": {
            "name": "Lead Time Metric",
            "description": "Get the Lead Time of a set of Work Items in a specific query",
            "previewImageUrl": "img/helloWorld.png",
            "uri": "LeadTime.html",
            "supportedSizes": [{
                "rowSpan": 1,
                "columnSpan": 1
            }],
            "supportedScopes": [
                "project_team"
            ]
        }
    }, {
        "id": "LeadTimeMetric.Configuration",
        "type": "ms.vss-dashboards-web.widget-configuration",
        "targets": [
            "ms.vss-dashboards-web.widget-configuration"
        ],
        "properties": {
            "name": "Lead Time Metric Configuration",
            "description": "Configures Lead Time Metric",
            "uri": "configuration.html"
        }
    }],
    "files": [{
        "path": "LeadTime.html",
        "addressable": true
    }, {
        "path": "configuration.html",
        "addressable": true
    }, {
        "path": "sdk/scripts",
        "addressable": true
    }, {
        "path": "js/KpiAgile.js",
        "addressable": true
    }, {
        "path": "js/polyfill.min.js",
        "addressable": true
    }, {
        "path": "js/Configuration.js",
        "addressable": true
    }, {
        "path": "img",
        "addressable": true
    }],
    "scopes": [
        "vso.work"
    ]
}