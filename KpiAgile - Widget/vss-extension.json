{
    "manifestVersion": 1,
    "id": "lead-time-extension",
    "version": "1.0.0",
    "name": "Lead Time Metric",
    "description": "Get the Lead Time of a set of Work Items in a specific query",
    "publisher": "brunosouza",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "contributions": [{
        "id": "LeadTimeMetric",
        "type": "ms.vss-dashboards-web.widget",
        "targets": [
            "ms.vss-dashboards-web.widget-catalog"
        ],
        "properties": {
            "name": "Lead Time Metric",
            "description": "Get the Lead Time of a set of Work Items in a specific query",
            "previewImageUrl": "img/helloWorld.png",
            "uri": "LeadTime.html",
            "supportedSizes": [{
                "rowSpan": 1,
                "columnSpan": 1
            }],
            "supportedScopes": [
                "project_team"
            ]
        }
    }],
    "files": [{
        "path": "LeadTime.html",
        "addressable": true
    }, {
        "path": "sdk/scripts",
        "addressable": true
    }, {
        "path": "js/KpiAgile.js",
        "addressable": true
    }, {
        "path": "js/polyfill.min.js",
        "addressable": true
    }, {
        "path": "img",
        "addressable": true
    }],
    "scopes": [
        "vso.work"
    ]
}